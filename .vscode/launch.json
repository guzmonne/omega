  "version": "0.2.0",
  "configurations": [
    {
      "name": "Omega chrome record",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/omega",
      "env": {
        "GO_ENV": "debug",
        "OMEGA_SERVER_ASSETS": "../../assets",
        "OMEGA_SERVER_TEMPLATES": "../../templates/*",
      },
      "args": ["chrome", "record"]
    },
    {
      "name": "Omega chrome fsm",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/omega",
      "env": {
        "GO_ENV": "debug",
        "OMEGA_SERVER_ASSETS": "../../assets",
        "OMEGA_SERVER_TEMPLATES": "../../templates/*",
      },
      "args": ["chrome", "channels"]
    },
    {
      "name": "Launch test package",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}"
    }
  ]
}